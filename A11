import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner num =new Scanner(System.in);
        
        System.out.println("enter a number");
        int n=num.nextInt();
        System.out.println("the table is");
        for(int i=1;i<=20;i++)
        {
            int k=n*i;
            System.out.println(n +"*" + i + "=" +k);
        }
    }
}
