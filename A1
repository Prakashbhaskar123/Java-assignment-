import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        int a,b;
        Scanner one=new Scanner(System.in);
        System.out.println("enter a");
        a=one.nextInt();
        System.out.println("enter b");
        b=one.nextInt();
        a=a+b;
        b=a-b;
        a=a-b;
    
        System.out.println(a);
        System.out.println(b);
    }
}
